algoritmo "semnome"
// Função :
// Autor :
// Data : 04/06/24
// Seção de Declarações 
var

   numero : inteiro

inicio

      Escreval ("Digie seu numero: ")
      Leia (numero)

      Se ( numero > 10 ) Entao
      
      Escreval (numero, "é MAIOR que 10.")
      
      Senao
      
      Escreval (numero, "é MENOR que 10.")
      
      FimSE
      
      

      

fimalgoritmo
