algoritmo "Lan House"
// Função :
// Autor :
// Data : 04/06/24
// Seção de Declarações 
var

   totalMinutos, totalAPagar : real


inicio


   escreval ("Digite total de minutos utilizados pelo cliente")
   leia (totalMinutos)

   totalAPagar <- totalMinutos * ((1/60)*2)
   
   escreval (" total a pagar R$ ", totalAPagar)


fimalgoritmo
